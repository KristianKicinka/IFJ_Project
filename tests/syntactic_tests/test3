require "ifj21"
function foo(x : integer, y : integer) : integer, integer
local i : integer = x
local j : integer = y
i, j = 1, i -- vyhodnocuj zprava a přiřazuj později
return i, j
end
function main()
local a : integer = 1
local b : integer = 2
a, b = foo(a, b) -- returns 13, 2
if b then
else
local a : integer = 666
end
write(a) 
end
main()

